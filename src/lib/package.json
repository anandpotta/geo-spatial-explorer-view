
{
  "name": "geospatial-explorer-lib",
  "version": "0.1.8",
  "description": "A comprehensive cross-platform geospatial library for React, Angular, and React Native applications",
  "main": "./dist/cjs/lib/index.js",
  "module": "./dist/esm/lib/index.js",
  "types": "./dist/types/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/lib/index.js",
      "require": "./dist/cjs/lib/index.js",
      "types": "./dist/types/lib/index.d.ts"
    },
    "./react": {
      "import": "./dist/esm/lib/react/index.js",
      "require": "./dist/cjs/lib/react/index.js",
      "types": "./dist/types/lib/react/index.d.ts"
    },
    "./angular": {
      "import": "./dist/esm/lib/angular/index.js",
      "require": "./dist/cjs/lib/angular/index.js",
      "types": "./dist/types/lib/angular/index.d.ts"
    },
    "./react-native": {
      "import": "./dist/esm/lib/react-native/index.js",
      "require": "./dist/cjs/lib/react-native/index.js",
      "types": "./dist/types/lib/react-native/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "node build.js",
    "build:cjs": "tsc --project ./tsconfig.build.json --module commonjs --outDir ./dist/cjs",
    "build:esm": "tsc --project ./tsconfig.build.json --module es2015 --outDir ./dist/esm",
    "build:types": "tsc --project ./tsconfig.build.json --declaration --declarationMap --declarationDir ./dist/types --emitDeclarationOnly",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "geospatial",
    "maps",
    "globe",
    "react",
    "angular",
    "react-native",
    "leaflet",
    "threejs",
    "cross-platform"
  ],
  "author": "Geospatial Explorer Team",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@angular/core": ">=12.0.0",
    "@angular/common": ">=12.0.0",
    "react-native": ">=0.60.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "three": "^0.133.1"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
