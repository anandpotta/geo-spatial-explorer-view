
{
  "name": "geospatial-explorer-lib",
  "version": "1.0.0",
  "description": "A cross-platform geospatial library supporting React, React Native, and Angular",
  "main": "./dist/cjs/lib/index.js",
  "module": "./dist/esm/lib/index.js",
  "types": "./dist/types/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/lib/index.js",
      "require": "./dist/cjs/lib/index.js",
      "types": "./dist/types/lib/index.d.ts"
    },
    "./react": {
      "import": "./dist/esm/lib/react/index.js",
      "require": "./dist/cjs/lib/react/index.js",
      "types": "./dist/types/lib/react/index.d.ts"
    },
    "./react-native": {
      "import": "./dist/esm/lib/react-native/index.js",
      "require": "./dist/cjs/lib/react-native/index.js",
      "types": "./dist/types/lib/react-native/index.d.ts"
    },
    "./angular": {
      "import": "./dist/angular/geospatial-explorer-lib.mjs",
      "require": "./dist/angular/bundles/geospatial-explorer-lib.umd.js",
      "types": "./dist/angular/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "node build.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "geospatial",
    "maps",
    "globe",
    "react",
    "react-native",
    "angular",
    "typescript",
    "leaflet",
    "threejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@angular/core": ">=12.0.0",
    "@angular/common": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    }
  },
  "dependencies": {
    "three": "^0.133.1",
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@types/three": "^0.133.0",
    "@types/leaflet": "^1.9.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/geospatial-explorer-lib.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/geospatial-explorer-lib/issues"
  },
  "homepage": "https://github.com/yourusername/geospatial-explorer-lib#readme"
}
