
{
  "name": "geospatial-explorer-lib",
  "version": "0.1.1",
  "description": "Cross-platform GeoSpatial Explorer library for React, React Native, and Angular",
  "main": "dist/cjs/lib/index.js",
  "module": "dist/esm/lib/index.js",
  "types": "dist/types/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/lib/index.js",
      "require": "./dist/cjs/lib/index.js",
      "types": "./dist/types/lib/index.d.ts"
    },
    "./react": {
      "import": "./dist/esm/lib/react/index.js",
      "require": "./dist/cjs/lib/react/index.js",
      "types": "./dist/types/lib/react/index.d.ts"
    },
    "./react-native": {
      "import": "./dist/esm/lib/react-native/index.js",
      "require": "./dist/cjs/lib/react-native/index.js",
      "types": "./dist/types/lib/react-native/index.d.ts"
    },
    "./angular": {
      "import": "./dist/esm/lib/angular/index.js",
      "require": "./dist/cjs/lib/angular/index.js",
      "types": "./dist/types/lib/angular/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "node build.js",
    "build:cjs": "tsc -p tsconfig.json --module commonjs --outDir dist/cjs --declaration false --rootDir ../../",
    "build:esm": "tsc -p tsconfig.json --module es2015 --outDir dist/esm --declaration false --rootDir ../../", 
    "build:types": "tsc -p tsconfig.json --declaration --declarationDir dist/types --emitDeclarationOnly --rootDir ../../",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "geospatial",
    "map",
    "globe",
    "3d",
    "cross-platform",
    "react",
    "react-native",
    "angular",
    "leaflet",
    "cesium",
    "three.js"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-native": ">=0.60.0",
    "react-native-webview": ">=11.0.0",
    "@angular/core": ">=13.0.0",
    "three": ">=0.133.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    },
    "react-native-webview": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "@types/react-native": ">=0.60.0",
    "@types/three": ">=0.133.0",
    "typescript": ">=4.5.0",
    "rimraf": "^3.0.2",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  }
}
