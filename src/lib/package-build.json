
{
  "name": "@your-org/geospatial-explorer",
  "version": "1.0.0",
  "description": "Cross-platform GeoSpatial Explorer library with React, Angular, and React Native support",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./react": {
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.js",
      "types": "./dist/types/react/index.d.ts"
    },
    "./angular": {
      "import": "./dist/esm/angular/index.js",
      "require": "./dist/cjs/angular/index.js",
      "types": "./dist/types/angular/index.d.ts"
    },
    "./react-native": {
      "import": "./dist/esm/react-native/index.js",
      "require": "./dist/cjs/react-native/index.js",
      "types": "./dist/types/react-native/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --declaration false",
    "build:esm": "tsc --module es2015 --outDir dist/esm --declaration false",
    "build:types": "tsc --declaration --declarationDir dist/types --emitDeclarationOnly",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "geospatial",
    "map",
    "globe",
    "3d",
    "cross-platform",
    "react",
    "react-native",
    "angular",
    "threejs",
    "leaflet",
    "cesium"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/geospatial-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/geospatial-explorer/issues"
  },
  "homepage": "https://github.com/your-org/geospatial-explorer#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-native": ">=0.60.0",
    "react-native-webview": ">=11.0.0",
    "@angular/core": ">=13.0.0",
    "@angular/common": ">=13.0.0",
    "three": ">=0.133.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "react-native": {
      "optional": true
    },
    "react-native-webview": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "@types/react-native": ">=0.60.0",
    "@types/three": ">=0.133.0",
    "@angular/core": ">=13.0.0",
    "@angular/common": ">=13.0.0",
    "typescript": ">=4.5.0",
    "rimraf": "^3.0.2",
    "jest": "^29.0.0",
    "eslint": "^8.0.0"
  }
}
